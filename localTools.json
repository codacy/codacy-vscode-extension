{
  "tools": [
    {
      "title": "Trivy",
      "cliCommand": "trivy",
      "cliExecute": "trivy fs [[PATH]] --quiet --format sarif -o ./.codacy/runs/trivy.sarif",
      "cliVersion": "trivy -v",
      "cli-InstallMacos": "brew install trivy"
    },
    {
      "title": "Semgrep",
      "cliCommand": "semgrep",
      "cliExecute": "semgrep scan [[PATH]] --quiet --sarif --output ./.codacy/runs/semgrep.sarif",
      "cliVersion": "semgrep --version",
      "cliInstallMacos": "brew install semgrep"
    },
    {
      "title": "Codacy-ESLint",
      "cliCommand": "codacy-cli-v2",
      "cliExecute": "codacy-cli-v2 analyze eslint --output ./.codacy/runs/eslint.sarif",
      "cliVersion": "1.0.0",
      "cliInstallMacos": "brew install codacy/codacy-cli-v2/codacy-cli-v2"
    }
  ]
}